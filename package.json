{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "octocast",
  "title": "octocast",
  "description": "Manage your OctoPrint server from Raycast",
  "icon": "command-icon.png",
  "author": "Jason Antwi-Appah <hey@jasonaa.me>",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show List",
      "description": "Template to show a list received from a networking response",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.6",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
